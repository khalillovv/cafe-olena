{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["prisma/schema.prisma"]
			}
		}
	],
	"hooks": {
		"postbuild": "prisma generate"
	},
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "DELETE"]
		}
	]
}
